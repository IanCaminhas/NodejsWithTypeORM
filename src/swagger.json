  "paths": {
    "/roles": {
      "post": {
        "tags": ["Role"],
        "summary": "Create a role",
        "description": "create a new role",
        "requestBody": {
          "content": {
            "applicatiom/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Role created.",
            "content": {
              "applicatiom/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type":"string"
                    },
                    "name": {
                      "type":"string"
                    },
                    "created_at": {
                      "type":"string"
                    }
                  }
                }
              }
            }

          },
          "400": {
            "description": "Role already exists."
          }
      }
    },
    "get": {
      "tags": ["Role"],
      "summary": "List all roles",
      "description": "Returns all roles paginated.",
      "responses": {
        "200": {
          "description": "Success."
        }
      }
    }
  }
}
